<interface>
  <!-- interface-name builder.ui -->
  <!-- interface-authors Fabio Grazioso -->
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkWindow" id="mainWindow">
    <property name="default-height">400</property>
    <property name="default-width">1000</property>
    <property name="has-resize-grip">True</property>
    <property name="title">VCF archives utility</property>
    <child>
      <object class="GtkGrid" id="grid_general">
        <property name="orientation">vertical</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkFrame" id="frame_top">
            <property name="border-width">2</property>
            <property name="margin">2</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkGrid" id="grid1">
                <property name="orientation">vertical</property>
                <property name="visible">True</property>
                <child>
                  <object class="GtkLabel" id="label_inputfile_1">
                    <property name="halign">start</property>
                    <property name="label">input path 1:</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label_instructions">
                    <property name="halign">start</property>
                    <property name="label">This application can merge two vcf files. The two input fields take the two paths of vcf files, the output fiield sets the output directory
                    </property>
                    <property name="margin">6</property>
                    <property name="visible">True</property>
                    <property name="wrap">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkEntry" id="entry_file_1">
                    <property name="has-tooltip">True</property>
                    <property name="height-request">1</property>
                    <property name="hexpand">True</property>
                    <property name="margin">1</property>
                    <property name="tooltip-text">this is an entry field</property>
                    <property name="visible">True</property>
                    <property name="width-request">4</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">2</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkButton" id="button_open_1">
                    <property name="halign">start</property>
                    <property name="label">sel. input 1</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">1</property>
                    <property name="top-attach">2</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkButton" id="button_open_3">
                    <property name="halign">start</property>
                    <property name="label">choose out folder</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">1</property>
                    <property name="top-attach">6</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkEntry" id="entry_out_folder">
                    <property name="has-tooltip">True</property>
                    <property name="height-request">1</property>
                    <property name="hexpand">True</property>
                    <property name="margin">1</property>
                    <property name="tooltip-text">In this field can be pasted the full path to the output folder.\n"
The path can be edited manually before the start of the processing."</property>
                    <property name="visible">True</property>
                    <property name="width-request">4</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">6</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label_output_folder">
                    <property name="halign">start</property>
                    <property name="label">output folder path:</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">5</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label_inputfile_2">
                    <property name="halign">start</property>
                    <property name="label">input path 2:</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">3</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkEntry" id="entry_file_2">
                    <property name="has-tooltip">True</property>
                    <property name="height-request">1</property>
                    <property name="hexpand">True</property>
                    <property name="margin">1</property>
                    <property name="tooltip-text">this is an entry field</property>
                    <property name="visible">True</property>
                    <property name="width-request">4</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">4</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkButton" id="button_open_2">
                    <property name="halign">start</property>
                    <property name="label">sel. input 2</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">1</property>
                    <property name="top-attach">4</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkEntry" id="entry_out_filename">
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="height">1</property>
                    <property name="left-attach">0</property>
                    <property name="top-attach">8</property>
                    <property name="width">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel" id="label_out_filename">
                    <property name="halign">start</property>
                    <property name="label">output filename:</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">7</property>
                  </packing>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkFrame" id="frame_operations">
            <property name="border-width">2</property>
            <property name="margin">2</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkGrid" id="grid_operations">
                <property name="visible">True</property>
                <child>
                  <object class="GtkButton" id="button_run">
                    <property name="halign">start</property>
                    <property name="label">Process</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">0</property>
                    <property name="top-attach">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkButton" id="button_quit">
                    <property name="halign">start</property>
                    <property name="label">Quit</property>
                    <property name="visible">True</property>
                  </object>
                  <packing>
                    <property name="left-attach">3</property>
                    <property name="top-attach">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkRadioButton" id="radio_merge">
                    <property name="label">merge</property>
                    <property name="visible">True</property>
                    <property name="group">radio_merge</property>
                    <signal name="toggled" handler="on_radio_merge_toggled" swapped="no"/>
                  </object>
                  <packing>
                    <property name="height">1</property>
                    <property name="left-attach">0</property>
                    <property name="top-attach">0</property>
                    <property name="width">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkRadioButton" id="radio_deduplicate">
                    <property name="label">deduplicate</property>
                    <property name="visible">True</property>
                    <property name="group">radio_merge</property>
                    <signal name="toggled" handler="on_radio_deduplicate_toggled" swapped="no"/>
                  </object>
                  <packing>
                    <property name="height">1</property>
                    <property name="left-attach">1</property>
                    <property name="top-attach">0</property>
                    <property name="width">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkRadioButton" id="radio_clean">
                    <property name="label">clean</property>
                    <property name="visible">True</property>
                    <property name="group">radio_merge</property>
                    <signal name="toggled" handler="on_radio_clean_toggled" swapped="no"/>
                  </object>
                  <packing>
                    <property name="height">1</property>
                    <property name="left-attach">2</property>
                    <property name="top-attach">0</property>
                    <property name="width">1</property>
                  </packing>
                </child>


              </object>
            </child>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkFrame" id="frame_bottom">
            <property name="border-width">2</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkScrolledWindow" id="scroll_win">
                <property name="visible">True</property>
                <child>
                  <object class="GtkTextView" id="textview_log">
                    <property name="border-width">1</property>
                    <property name="editable">False</property>
                    <property name="expand">True</property>
                    <property name="visible">True</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkAdjustment" id="adj_strt">
    <property name="page-increment">5.0</property>
    <property name="step-increment">1.0</property>
    <property name="upper">99999.0</property>
  </object>
  <object class="GtkAdjustment" id="adj_end">
    <property name="page-increment">5.0</property>
    <property name="step-increment">1.0</property>
    <property name="upper">99999.0</property>
  </object>
</interface>
